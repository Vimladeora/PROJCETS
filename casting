    "students_data = [\n",
    "    [\"101\", \"Vimla\", \"21\", \"85.5\"],\n",
    "    [\"102\", \"Deora\", \"22\", \"90.0\"],\n",
    "    [\"103\", \"Rahul\", \"20\", \"78.2\"],\n",
    "    [\"104\", \"Priya\", \"21\", \"88.7\"]\n",
    "]\n",
    "\n",
    "\n",
    "students = []\n",
    "for s in students_data:\n",
    "    student = {\n",
    "        \"id\": int(s[0]),            # string → int\n",
    "        \"name\": s[1],               # keep as string\n",
    "        \"age\": int(s[2]),           # string → int\n",
    "        \"marks\": float(s[3])        # string → float\n",
    "    }\n",
    "    students.append(student)\n",
    "\n",
    "\n",
    "print(\"After Casting:\")\n",
    "for s in students:\n",
    "    print(s)\n",
    "\n",
    "\n",
    "total_marks = sum(s[\"marks\"] for s in students)\n",
    "avg_marks = total_marks / len(students)\n",
    "print(\"\\nAverage Marks:\", avg_marks)\n",
    "\n",
    "\n",
    "topper = max(students, key=lambda x: x[\"marks\"])\n",
    "print(\"Topper:\", topper[\"name\"], \"with\", topper[\"marks\"], \"marks\")\n",
    "\n"
   ]
